.index_swiper swiper {
  width: 750rpx;
  height: 340rpx;
  padding: 10rpx;
}
.index_swiper swiper image {
  width: 100%;
  border-radius: 15rpx;
}
.index_floor .floor_group .floor_title {
  padding: 20rpx;
  font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
  font-weight: 700;
  font-size: 50rpx;
  color: #1d1d1d;
  text-decoration: none;
}
.index_floor .floor_group .floor_list navigator {
  float: left;
  margin-left: 30rpx;
  margin-bottom: 30rpx;
}
.index_floor .floor_group .floor_list navigator image {
  overflow: hidden;
  border-color: #bbbbbb;
  border-width: 0px;
  border-style: solid;
  border-radius: 8px;
  width: 330rpx;
}
.index_floor .floor_group .floor_list navigator .recommend {
  width: 320rpx;
  font-family: PingFangSC;
  font-weight: 700;
  font-size: 30rpx;
  color: #444444;
  font-style: normal;
  letter-spacing: 0px;
  line-height: 30rpx;
  text-decoration: none;
}
